
{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:react/jsx-runtime"
    ],
    "plugins": ["react", "import", "jsx-a11y", "@typescript-eslint"],
    "rules": {
        "react/prop-types": 0,
        "indent": ["error", 2]
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
        "jsx": true
        }
    },
    "env": {
        "es2021": true,
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
        "version": "detect"
        }
    }
}
    